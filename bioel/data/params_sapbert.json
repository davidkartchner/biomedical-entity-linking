{
    "model_dir": "microsoft/BiomedNLP-PubMedBERT-base-uncased-abstract-fulltext",
    "train_dir": "data/sapbert/ncbi_disease/ncbi_disease_aliases.txt",
    "output_dir": "data/sapbert/ncbi_disease/finetuned_models_nice",
    "dataset_name": "ncbi_disease",
    "use_cuda": true,
    "learning_rate": 2e-5,
    "train_batch_size": 256,
    "eval_batch_size": 32,
    "split": "test",
    "debug": true,
    "epoch": 3,
    "save_checkpoint_all": false,
    "checkpoint_step": 10000000,
    "amp": true,
    "parallel": false,
    "pairwise": true,
    "random_seed": 33,
    "loss": "ms_loss",
    "use_miner": true,
    "miner_margin": 0.2,
    "type_of_triplets": "all",
    "agg_mode": "cls",
    "num_workers": 16,
    "mode": "finetune",
    "project": "SAPBERT",
    "resolve_abbreviations": true,
    "path_to_abbreviation": "data/abbreviations.json",
    "dict_cache_path": "data/sapbert/ncbi_disease/ncbi_disease_dict.pt"
}